{
  "manifest_version": 2,
  "name": "Linked List Extension",
  "description": "This extension saves links directly to Linked List",
  "version": "1.0",
  "permissions": [
    "tabs",
    "http://*/*",
    "https://*/*"
  ],
  "browser_action": {
    "default_icon": "icon.png",
    "default_popup": "popup.html",
    "default_title": "Save link to Linked List"
  }
}